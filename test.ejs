<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>School Admin - School Settings</title>
    <!-- Bootstrap core CSS-->
    <link href="../../../dashboard_public/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template-->
   
<link rel="stylesheet" href="../../../dashboard_public/vendor/fontawesome-free/css/all.min.css">
    <!-- Page level plugin CSS-->

    <!-- Custom styles for this template-->
    <link href="../../../dashboard_public/css/sb-user.css" rel="stylesheet">
</head>
<style type="text/css">
 
body {
  background-color: navajowhite;
}
  </style>
<body id="page-top">

    <nav class="navbar navbar-expand navbar-dark bg-dark static-top">

      <a class="navbar-brand mr-1" href="/user/dashboard">Menu</a>

      <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
        <i class="fas fa-bars"></i>
      </button>

      <!-- Navbar Search -->
      <form class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">
        
      </form>

      <!-- Navbar -->
   
     
    </nav>
    <div id="wrapper_two">
    </div>
    <div id="wrapper">

      <!-- Sidebar -->
      
      <ul class="sidebar navbar-nav">
        <li class="nav-item active">
          <a class="nav-link" href="/user/dashboard">
            <i class="fas fa-fw fa-tachometer-alt"></i>
            <span>Dashboard</span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/user/register-student">
            <i class="fas fa-address-card"></i>
            <span>Register a Student</span>
          </a>
        </li>
   
              
        <li class="nav-item">
          <a class="nav-link" href="/user/tables">
            <i class="fas fa-fw fa-table"></i>
            <span>Registered Students</span></a>
        </li>
      
        <li class="nav-item">
          <a class="nav-link" href="/user/pending-school-fee">
            <i class="fas fa-user-circle"></i>
            <span>General Pending Arrears</span></a>
        </li>
        
        <li class="nav-item">
          <a class="nav-link" href="/user/student-data-sheet">
            <i class="fas fa-fw fa-table"></i>
            <span>School Data Sheet</span></a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="/user/school-settings">
            <i class="fas fa-users-cog"></i>
            <span>School Settings</span></a>
        </li>
       

        <li class="nav-item">
          <a class="nav-link" href="/user/logout">
            <i class="fa fa-sign-out-alt"></i>
            <span>Logout</span></a>
        </li>
      </ul>

      <div id="content-wrapper">
             <!-- /.container-fluid -->
             <%   if(typeof message != "undefined" && message) { %>

                <div class="mt-5 mb-3">
        
                  <div class="alert alert-primary alert-dismissible fade show" role="alert">
        
                    <h2><strong><%=  message  %></strong></h2>
        
                    <button type="button" class="close" data-dismiss="alert">&times;</button>
        
                  </div>
        
                </div> 
        
              <% }    %>
             <div class="container mt-3 w-100">
              <form>
                <div class="form-group">
                  <label for="title">Title</label>
                  <input type="email" class="form-control" id="title" placeholder="Enter Title">
                </div>
                <br>
                <textarea name="content" id="body" cols="145" rows="25" class="form-control"></textarea>
                
              
              </form>
              </div>
                       
              
        </div>
        <!-- Sticky Footer -->
        <footer class="sticky-footer">
          <div class="container my-auto">
            <div class="copyright text-center my-auto">
              <span>Copyright Â© My School 2022</span>
            </div>
          </div>
        </footer>

      </div>
      <!-- /.content-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
      <i class="fas fa-angle-up"></i>
    </a>
 
    
  
    <!-- Bootstrap core JavaScript-->
    <script src="../../../dashboard_public/vendor/jquery/jquery.min.js"></script>
